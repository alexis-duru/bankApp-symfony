<form method="post" action="{{ path('profil_delete', {'id': app.user.id}) }}" onsubmit="return confirm('Are you sure you want to delete your account?');">
	<input type="hidden" name="_token" value="{{ csrf_token('delete' ~ app.user.id) }}">
	<button class="btn btn-delete">Supprimer mon profil
	</button>
</form>
